<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" class="form-control" autocomplete="off">
        <label>Post</label>
        <textarea name="postBody" cols="30" rows="5" class="form-control" autocomplete="off"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") %>
